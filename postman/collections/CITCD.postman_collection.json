          "event": [
            {
              "listen": "test",
              "script": {
                "id": "05883c7b-7722-4c68-916e-fada38fe6bba",
                "exec": [
                  "const responseJson = pm.response.json();\r",
                  "\r",
                  "const accessToken = responseJson.accessToken;\r",
                  "\r",
                  "if (accessToken) {\r",
                  "    pm.environment.set(\"accessToken\", accessToken);\r",
                  "    pm.globals.set(\"accessToken\", accessToken);\r",
                  "    console.log(\"JWT saved:\", accessToken);\r",
                  "} else {\r",
                  "    console.log(\"No token found in response body\");\r",
                  "}"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
